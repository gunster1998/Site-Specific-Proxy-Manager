{
  "manifest_version": 2,
  "name": "Proxy Extension",
  "version": "1.0",
  "permissions": [
      "proxy",
      "storage",
      "tabs",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
  ],
  "background": {
      "scripts": ["background.js"],
      "persistent": true
  },
  "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
          "16": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
      }
  },
  "options_ui": {
      "page": "options.html",
      "open_in_tab": true
  },
  "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
  }
}
